using System;
					
public class unit1aChallenge
{
	public static void Main()
	{
		//interger
		int myNum = 12;
		//extend challange
		int mySecondNum = 4;
		int myThirdNum = 8;
		//Floating-Point
		double myFloat = 12.5;
		double myFloat2 = 6.5;
		
		//boolean
		bool myBool = true;
		//string
		string sayThis = "tekken 8";
		string sayThat = "Azucina is my main";
		
		//display que
		
		Console.WriteLine("this is an interger " + myNum);
		Console.WriteLine("This is a Floating Point " + myFloat);
		Console.WriteLine("This is a boolean Value " + myBool);
		Console.WriteLine("This game is " + sayThis);
		
		//extend challange operations and basic arithmetic
		
		Console.WriteLine("in " + sayThis +" " + sayThat + "."); //concatination requires that I add the space in as well
		Console.WriteLine(myNum * mySecondNum / myThirdNum);
		Console.WriteLine(myFloat2 - myFloat);
		
		//Challenge 3 User input!
		
		Console.WriteLine("Write a number!");
		int yourNum1 = Convert.ToInt32(Console.ReadLine());
		Console.WriteLine("Wrte another number.");
		int yourNum2 = Convert.ToInt32(Console.ReadLine());
		Console.WriteLine("Tell me your name");
		string name = Console.ReadLine();
		int x = yourNum1;
		int y = yourNum2;
		int Sum1 = x + y;
		Console.WriteLine("Character Name " + name);
		Console.WriteLine("Your numbers added are " + Sum1);
		
	}
}